        super();
    }

    public void update()
    {
        super.update();
        if (!getObjects(Player.class).isEmpty())
        {
            Player player = getObjects(Player.class).get(0);
            if (player.getTrueX() > Options.WIDTH - 2)
            {
                Room2 room = new Room2();
                switchRoom(room, player, 2, player.getTrueY());
            }
        }
    }

